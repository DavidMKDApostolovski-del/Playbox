<script>
// High Scores (localStorage)
let highscores = JSON.parse(localStorage.getItem('highscores')) || {};
function saveScore(game, score){
  if(!highscores[game] || score > highscores[game]){
    highscores[game] = score;
    localStorage.setItem('highscores', JSON.stringify(highscores));
    alert('ğŸ† New High Score: ' + score);
  }
}
function showScores(){
  let msg='ğŸ† High Scores
';
  for(const g in highscores){msg+=`${g}: ${highscores[g]}
`;}
  alert(msg || 'No scores yet');
}

function toggleFullscreen(){
  if(!document.fullscreenElement){document.documentElement.requestFullscreen();}
  else{document.exitFullscreen();}
}

// Street Dunk Duel (original)
let dunkInt;
function startDunk(){
  const c=document.getElementById('dunk'); const x=c.getContext('2d');
  let pY=110,bX=130,bY=20,v=0,score=0;
  c.onclick=()=>v=-6;
  clearInterval(dunkInt);
  dunkInt=setInterval(()=>{
    x.clearRect(0,0,c.width,c.height);
    v+=0.4; pY+=v; if(pY>110){pY=110;v=0}
    bY+=2; if(bY>150){bY=20; bX=Math.random()*200+30}
    if(Math.abs(bX-40)<15 && Math.abs(bY-pY)<15){score++; bY=20}
    x.fillStyle='#38bdf8'; x.fillRect(30,pY,20,30);
    x.beginPath(); x.arc(bX,bY,6,0,7); x.fill();
    x.fillText('Score '+score,180,15); saveScore('Street Dunk Duel', score);
  },16);
}

// Bobble Soccer Stars (original)
let socInt;
function startSoccer(){
  const c=document.getElementById('soccer'); const x=c.getContext('2d');
  let p=80,ai=80,bx=130,by=80,dx=2,dy=1;
  c.onmousemove=e=>{const r=c.getBoundingClientRect(); p=e.clientY-r.top;}
  clearInterval(socInt);
  socInt=setInterval(()=>{
    x.clearRect(0,0,c.width,c.height);
    by+=dy; bx+=dx;
    if(by<0||by>160)dy*=-1;
    if(bx<20&&Math.abs(by-p)<20)dx*=-1;
    if(bx>240&&Math.abs(by-ai)<20)dx*=-1;
    ai+=((by-ai)*0.05);
    x.beginPath(); x.arc(15,p,14,0,7); x.arc(245,ai,14,0,7); x.fill();
    x.beginPath(); x.arc(bx,by,5,0,7); x.fill();
    if(bx<0||bx>260){bx=130;by=80}
  },16);
}

// Reaction Test
let reactionTime;
function reactionStart() {
  const result = document.getElementById('reactionResult');
  result.innerText = 'Wait for green...';
  const delay = Math.random() * 3000 + 1000;
  setTimeout(() => {
    reactionTime = Date.now();
    document.body.style.background = '#16a34a';
    document.body.onclick = () => {
      const time = Date.now() - reactionTime;
      result.innerText = 'Your reaction time: ' + time + ' ms';
      document.body.style.background = '#0f172a';
      document.body.onclick = null;
    };
  }, delay);
}

// Click Rush
let clickScore = 0;
let clickActive = false;
function startClickRush(){document.getElementById('clickSound').play();() {
  clickScore = 0;
  clickActive = true;
  document.getElementById('clickScore').innerText = 'Score: 0';
  const btn = document.createElement('button');
  btn.innerText = 'CLICK!';
  btn.onclick = () => {
    if (clickActive) {
      clickScore++;
      document.getElementById('clickScore').innerText = 'Score: ' + clickScore;
    }
  };
  document.querySelector('.card').appendChild(btn);
  setTimeout(() => {
    clickActive = false;
    btn.remove();
    alert('Time! Final score: '+clickScore); saveScore('Click Rush', clickScore);
  }, 10000);
}

// Number Guess
const secret = Math.floor(Math.random() * 100) + 1;
function checkGuess() {
  const val = Number(document.getElementById('guessInput').value);
  const result = document.getElementById('guessResult');
  if (val === secret) result.innerText = 'ğŸ‰ Correct!';
  else if (val > secret) result.innerText = 'Too high!';
  else result.innerText = 'Too low!';
}

// Mini Pong
let pongInterval;
function startPong() {
  const canvas = document.getElementById('pong');
  const ctx = canvas.getContext('2d');
  let paddleY = canvas.height / 2 - 20;
  let ballX = canvas.width / 2;
  let ballY = canvas.height / 2;
  let dx = 2, dy = 2;

  canvas.onmousemove = e => {
    const rect = canvas.getBoundingClientRect();
    paddleY = e.clientY - rect.top - 20;
  };

  clearInterval(pongInterval);
  pongInterval = setInterval(() => {
    ctx.clearRect(0,0,canvas.width,canvas.height);
    ctx.fillStyle = '#38bdf8';
    ctx.fillRect(10, paddleY, 6, 40);
    ctx.beginPath();
    ctx.arc(ballX, ballY, 4, 0, Math.PI*2);
    ctx.fill();
    ballX += dx; ballY += dy;
    if (ballY < 0 || ballY > canvas.height) dy *= -1;
    if (ballX < 16 && ballY > paddleY && ballY < paddleY + 40) dx *= -1;
    if (ballX > canvas.width) { ballX = canvas.width/2; }
  }, 16);
}
</script>
